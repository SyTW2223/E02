{
  "name": "pan",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/server",
    "dev": "nodemon",
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "testCoverage": "jest --coverage"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.4",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/koa": "^2.13.5",
    "@types/koa-basic-auth": "^2.0.4",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-mount": "^4.0.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-basic-auth": "^4.0.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "mongoose": "^6.7.2"
  },
  "main": "dist/server.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/src/db/**",
      "!**/src/server.ts",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage/"
    ]
  }
}
